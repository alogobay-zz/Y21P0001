{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["warning", {
      "endOfLine": "auto",
      "indent": [2,"tab"]
    }],
    "import/no-extraneous-dependencies": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": "off"
  }
}
